{
    "compilerOptions": {
        "jsx": "preserve",
        "target": "ES6",
        "module": "ESNext",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strictNullChecks": true,
        "lib": [
        "dom",
        "dom.iterable",
        "esnext"
        ],
        "allowJs": true,
        "strict": false,
        "noEmit": true,
        "incremental": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "plugins": [
        {
            "name": "next"
        }
        ]
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
